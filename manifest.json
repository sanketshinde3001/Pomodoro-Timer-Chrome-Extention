{
    "manifest_version": 3,
    "name": "Pomodoro Timer",
    "version": "1.0.0",
    "description": "Save Time and Utilize it for self-development",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "action": {
        "default_icon": {
            "16": "icons/16.png",
            "32": "icons/32.png",
            "48": "icons/48.png",
            "128": "icons/128.png"
        }
    },
    "permissions": ["storage","contextMenus","alarms","notifications"],
    "background": {
        "service_worker": "service_worker.js"
    }
}