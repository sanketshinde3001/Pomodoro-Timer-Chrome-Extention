{
    "manifest_version": 3,
    "name": "Pomodoro Timer",
    "version": "1.0.0",
    "description": "Save Time and Utilize it for self-development",
    "icons": {
        "24": "icons/24.png",
        "48": "icons/48.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "24": "icons/24.png",
            "48": "icons/48.png"
        }
    },
    "permissions": [
        "storage",
        "contextMenus",
        "alarms",
        "notifications"
    ],
    "background": {
        "service_worker": "service_worker.js"
    }
}